<Application x:Class="CashmaticApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CashmaticApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="IconsDictionary"  Source="pack://application:,,,/CashmaticApp;component/Icons.xaml" />
                <ResourceDictionary x:Name="ControlStyles"  Source="pack://application:,,,/CashmaticApp;component/ControlStyles.xaml" />
                <ResourceDictionary x:Name="EnglishLanguage" Source="pack://application:,,,/CashmaticApp;component/Languages/English.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

        
        
    </Application.Resources>
</Application>
